{
    "name": "needs_ontology",
    "version": "1.0.0",
    "license": "research-use",
    "created_for": "NVC-HH dataset (OFNR extraction and hallucination mitigation)",
    "sources": [
        "Marshall Rosenberg CNVC needs lists",
        "Manfred Max-Neef Fundamental Human Needs Matrix",
        "Operational semantic inventory supplied by Gemini-research"
    ],
    "description": "Canonical universal human needs ontology used for the field `need` ONLY. This ontology is locked. The labeler must choose needs from this list. Explicit_needs and implicit_needs are free-form and may contain strategies/objects. Use this ontology to map strategies to universal drivers.",
    "rules": {
        "need_field_is_canonical_only": true,
        "explicit_needs_free_form": true,
        "implicit_needs_free_form": true,
        "no_strategies_in_canonical_needs": true,
        "no_plato_elements_in_canonical_needs": true,
        "output_normalization": {
            "lowercase": true,
            "spaces_to_underscore": true
        }
    },
    "max_neef_fundamental_needs": [
        "subsistence",
        "protection",
        "affection",
        "understanding",
        "participation",
        "leisure",
        "creation",
        "identity",
        "freedom"
    ],
